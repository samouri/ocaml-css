{
  "name": "css-parse",
  "version": "0.1.0",
  "description": "Parse CSS",
  "license": "MIT",
  "esy": {
    "build": [
      "dune build",
      "dune build --profile release js_export/Index.bc.js"
    ],
    "install": [
      "dune install --prefix=#{self.install}"
    ],
    "release": {
      "releasedBinaries": [ "css-parse", "index.js" ]
    },
    "buildsInSource": "_build"
  },
  "resolutions": {
    "**/@opam/lwt": "4.0.0",
    "**/@opam/lwt_log": "1.1.0"
  },
  "dependencies": {
    "@esy-ocaml/esy-installer": "^0.0.0",
    "@opam/jbuilder": "*",
    "@opam/js_of_ocaml": "3.2.0",
    "@opam/js_of_ocaml-ppx": "3.2.0",
    "@opam/menhir": "20180703",
    "@opam/cmdliner": "1.0.2",
    "@opam/ppx_deriving": "^4.2.1",
    "@opam/ppx_deriving_yojson": "3.1",
    "@opam/yojson": "1.4.0",
    "@opam/ounit": "2.0.8"
  },
  "devDependencies": {
    "@opam/merlin": "^3.0.3",
    "ocaml": "~4.6.0"
  },
  "scripts": {
    "test": "make test"
  }
}
